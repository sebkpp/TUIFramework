         </Connector>
         <Connector>
            <SrcAddress entityName="MouseDemux1" entityType="MSP" portName="b1left"/>
            <DstAddress entityName="CustomDevice" entityType="TUI" portName="button_0"/>
         </Connector>
         <Connector>
            <SrcAddress entityName="MouseDemux1" entityType="MSP" portName="b2right"/>
            <DstAddress entityName="CustomDevice" entityType="TUI" portName="button_1"/>
         </Connector>
      </ConnectorSequence>
   </ServerConfig>
